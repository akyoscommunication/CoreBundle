{
  "aliases": [
    "akyos-core"
  ],
  "bundles": {
    "Akyos\\CoreBundle\\AkyosCoreBundle": ["all"],
    "Stof\\DoctrineExtensionsBundle\\StofDoctrineExtensionsBundle": ["all"],
    "Symfony\\Bundle\\SwiftmailerBundle\\SwiftmailerBundle": ["all"],
    "Knp\\Bundle\\PaginatorBundle\\KnpPaginatorBundle": ["all"],
    "FOS\\CKEditorBundle\\FOSCKEditorBundle": ["all"],
    "Doctrine\\Bundle\\FixturesBundle\\DoctrineFixturesBundle": ["dev", "test"],
    "Liip\\TestFixturesBundle\\LiipTestFixturesBundle": ["dev", "test"],
    "Nelmio\\Alice\\Bridge\\Symfony\\NelmioAliceBundle": ["dev", "test"],
    "Fidry\\AliceDataFixtures\\Bridge\\Symfony\\FidryAliceDataFixturesBundle": ["dev", "test"],
},
  "container": {
    "locale": "fr"
  },
  "copy-from-package": {
  },
  "env": {
    "MAILER_URL": "smtp://localhost:1025?encryption=&auth_mode=login&username=&password="
  },
  "composer-scripts": {
    "ckeditor:install": "symfony-cmd",
    "assets:install --symlink": "symfony-cmd",
    "yarn && yarn run encore dev": "script",
    "symfony server:start": "script"
  },
  "gitignore": [
    ".DS_Store",
    ".idea",
    ".vs"
  ]
}