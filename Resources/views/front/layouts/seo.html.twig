{% block title %}
    {% if seo is defined and seo is not null and seo.metaTitle is not null %}
        {{ seo.metaTitle }}
    {% else %}
        {{ parent() }}
    {% endif %}
{% endblock %}

{% block meta %}
    {% if seo is defined and seo is not null and seo.metaDescription is not null %}
        <meta name="description" content="{{ seo.metaDescription }}">
    {% endif %}
{% endblock %}
